{
    "version": 2,
    "builds": [
        {
            "src": "./api/app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        },
        {
            "src": "/(.*)",
            "dest": "/index"
        },
        {
            "src": "/(.*)",
            "dest": "/result"
        },
        {
            "src": "/(.*)",
            "dest": "/generate"
        }
    ]
}